<div class="row">
  <div class="col-md-10 col-md-offset-1">
  <% @order.errors.full_messages.each do |m| %>
    <%= m %>
  <% end %>
  <div>
    <%= form_for @order do |f|%>
        
      <%= f.submit class: "btn btn-lg btn-grey1", value: "Submit Your Order"%><br/>
        <%= f.hidden_field :street_number %>

      	<%= f.hidden_field :street %>

      	<%= f.hidden_field :city %>

      	<%= f.hidden_field :state %>

      	<%= f.hidden_field :zip %>

      	<%= f.hidden_field :status %>
      	<%= f.hidden_field :latitude %>
      	<%= f.hidden_field :longitude %>
        
      	<%= f.hidden_field :pickup_location %>
      	<%= f.hidden_field :pickup_date %>

      	<%= f.hidden_field :exchange %>
    <% end %>
  </div>
  </div>
</div>

