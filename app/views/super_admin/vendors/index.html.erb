<div class="container">
  <h1>New Vendor Requests</h1>

  <table class="table table-striped">
    <thead>
      <tr>
        <th>Name</th>
        <th>Description</th>
        <th>URL</th>
        <th>Approved?</th>
      </tr>
    </thead>
    <tbody>
        <% @vendors.each do |vendor| %>
        <tr>
          <td><%= vendor.name %></td>
          <td><%= vendor.description %></td>
          <td><%= vendor.slug %></td>
          <td><%= link_to "No", edit_super_admin_vendor_path(vendor) %></td>
          <td>
        </td>
      </tr>
      <% end %>
    </tbody>
  </table>
</div>
