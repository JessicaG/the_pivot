<h1>Current List of Vendors</h1>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th>URL</th>
      <th>Active?</th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <% @vendors.each do |vendor| %>
        <td><%= vendor.name %>&nbsp;<%= vendor.name %></td>
        <td><%= vendor.description %></td>
        <td><%= vendor.slug %></td>
        <td><%= link_to vendor.active, edit_super_admin_vendor_path(vendor) %></td>
        <td>
      </td>
    </tr>
    <% end %>
  </tbody>
</table>
