<div class="container">
  <h1>Create A New Vendor</h1>
  <h3>All fields are required</h3>
  <div class="row">
    <div class="col-md-1 col-md-offset-5">

      <%= form_for @vendor, :url => super_admin_vendors_path do |f| %>
        <%= f.label :name %>
        <%= f.text_field :name %>

        <%= f.label :description %>
        <%= f.text_field :description %>

        <%= f.submit "Create New Vendor" %>
      <% end %>
    </div>
  </div>
</div>
