<div class="jumbotron">
  <div class="container">
    <h2>Emergency Supplies</h2>
    <p style="color:darkred">when you need them most</p>
  </div>
</div>

    <div class="container marketing">
      <h1 style="text-align:center">Most Popular Menu Items</h1><br/>
      <% @top_three.each do |item| %>
        <div class="row">
        <div class="col-lg-4">
          <%= image_tag item.image.url(:medium), class: "img-circle featurette" if item.image_file_name %>
          <h2><%= item.title %></h2>
          <p><%= item.description %></p>
          <p><%= link_to "View details »", item_path(item), class: "btn btn-default" %></p>
        </div>
      <% end %>
    </div>

      <div id="footer">
        <div class="container">
        <p class="pull-right"><a href="#">Back to top</a></p>
        <p>© 2014 AirLift, Inc. · <a href="#">Privacy</a> · <a href="#">Terms</a></p>
        </div>
      </div>

    </div>


