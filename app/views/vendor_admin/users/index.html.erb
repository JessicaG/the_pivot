<h1>Current List of Users</h1>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Name</th>
      <th>Nickname</th>
      <th>Email</th>
      <th>Current Role (click to edit)</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
    <tr id="row-<%=user.id%>" >
        <td><%= user.first_name %>&nbsp;<%= user.last_name %></td>
        <td><%= user.nickname %></td>
        <td><%= user.email %></td>
        <td><%= link_to user.role, edit_vendor_admin_user_path(user) %></td>
    </tr>
    <% end %>
  </tbody>
</table>
