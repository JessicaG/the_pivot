<%= form_for(@user) do |f|  %>
  <%= f.label :first_name  %>
  <%= f.text_field :first_name  %>

  <%= f.label :last_name  %>
  <%= f.text_field :last_name  %>

  <%= f.label :nickname  %>
  <%= f.text_field :nickname  %>

  <%= f.label :email  %>
  <%= f.text_field :email  %>

  <%= f.label :role %>
  <%= f.text_field :role %>

  <%= f.submit  %>

<% end %>
